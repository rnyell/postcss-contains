{
  "name": "postcss-contains",
  "version": "0.1.0",
  "description": "A PostCSS plugin to styles selectors based on specific declaration or property.",
  "keywords": [
    "postcss",
    "postcss-plugin"
  ],
  "homepage": "https://github.com/rnyell/postcss-contains",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rnyell/postcss-contains.git"
  },
  "bugs": {
    "url": "https://github.com/rnyell/postcss-contains/issues"
  },
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "test": "vitest run",
    "ci": "npm run build && npm run check-format && npm run test"
  },
  "peerDependencies": {
    "postcss": "^8.4.47"
  },
  "devDependencies": {
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4"
